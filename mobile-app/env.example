# Trink Driver Mobile App Environment Variables

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000
EXPO_PUBLIC_WS_URL=ws://localhost:3000

# App Configuration
EXPO_PUBLIC_APP_NAME=Trink Driver
EXPO_PUBLIC_APP_VERSION=1.0.0

# Google Maps (if needed for enhanced maps)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Notification Configuration
EXPO_PUBLIC_NOTIFICATION_ICON=./assets/notification-icon.png
EXPO_PUBLIC_NOTIFICATION_COLOR=#dc2626

# Build Configuration
EXPO_PUBLIC_BUILD_ENV=development
EXPO_PUBLIC_DEBUG_MODE=true
